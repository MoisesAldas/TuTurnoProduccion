{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.lint.unknownAtRules": "ignore",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.inlayHints.enabled": "on",
  "sonarlint.rules": {
    "typescript:S1134": {
      "level": "on"
    },
    "typescript:S1135": {
      "level": "on"
    }
  },
  "sonarlint.connectedMode.project": {
    "projectKey": "tuturno_tuturno-produccion"
  }
}
